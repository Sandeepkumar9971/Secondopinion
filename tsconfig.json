{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/config/dbConfig.js", "src/app/api/auth/[...nextauth]/options.js", "tailwind.config.js", "src/components/CategorySearch.tsx", "src/context/dashboard/statereducer.js", "src/components/MultilistSelector.sxj", "src/app/dashboard/doctor/settings/add_details/add_detailsform.jsx"],
  "exclude": ["node_modules"]
}
